    create table expense (
        expense_id bigserial not null,
        active_flag integer,
        created_by_ip varchar(255) not null,
        created_date timestamp(6) not null,
        updated_by_ip varchar(255),
        updated_date timestamp(6),
        amount numeric(38,2) not null,
        entry_type varchar(255) check (entry_type in ('CASH_IN','CASH_OUT')),
        expense_date_time timestamp(6) not null,
        expense_name varchar(255) not null,
        remarks varchar(255),
        cashbook_id bigint not null,
        category_id bigint not null,
        payment_mode_id bigint not null,
        primary key (expense_id)
    )


    alter table if exists expense
       drop constraint if exists UKja85srdkqb9ln06eaxj6wwuql

    alter table if exists expense
       add constraint UKja85srdkqb9ln06eaxj6wwuql unique (expense_name, cashbook_id)

    alter table if exists expense
       add constraint FKaua84y9g2tpthei07j01oa9mu
       foreign key (cashbook_id)
       references cashbook

    alter table if exists expense
       add constraint FKmvjm59reb5i075vu38bwcaqj6
       foreign key (category_id)
       references category

    alter table if exists expense
       add constraint FK85ncqrt6wggc0xfvan16rfl9c
       foreign key (payment_mode_id)
       references payment_mode